{
  "private": true,
  "name": "sugggest-monorepo",
  "license": "MIT",
  "scripts": {
    "clean": "git clean -fdX .",
    "test": "jest",
    "dev": "yarn workspace @sugggest/core run build && yarn workspace @sugggest/remix-app run dev",
    "build": "yarn workspaces run build",
    "postinstall": "remix setup node",
    "migrate": "yarn workspace @sugggest/core run migrate",
    "studio": "yarn workspace @sugggest/core run studio"
  },
  "workspaces": [
    "packages/core",
    "packages/remix-app"
  ],
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^25.2.3",
    "@types/node-fetch": "^2.5.7",
    "jest": "^26.0.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=14"
  }
}
